    {
      "parameters": {
        "command": "=curl -v --max-time 600 -X POST http://ollama:11434/api/generate -d @{{ $json.ollamaRequestPath }}"
      },
      "id": "22222222-ai02-node-call-ollama",
      "name": "Call Ollama",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [
        11680,
        4112
      ]
    },
    {
      "parameters": {
        "jsCode": "try {\n  const stdout = $input.first().json.stdout;\n  const response = JSON.parse(stdout);\n  let content = response.response;\n  // Strip markdown code blocks if present\n  content = content.replace(/^```json\\s*/, '').replace(/\\s*```$/, '');\n  const evaluation = JSON.parse(content);\n  return { json: { evaluation, ...$('Parse Merge Output').first().json } };\n} catch(e) {\n  return { json: { error: e.message, raw: $input.first().json } };\n}"
      },
