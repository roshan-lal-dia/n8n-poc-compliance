    {
      "parameters": {
        "command": "=python3 /scripts/merge_hybrid.py \"{{ $json.fullPath }}\""
      },
      "id": "execute-merge-script",
      "name": "Execute Merge Script",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [
        10880,
        4112
      ]
    },
    {
      "parameters": {
        "jsCode": "const stdout = $input.first().json.stdout;\ntry {\n  const result = JSON.parse(stdout);\n  if (result.error) {\n    throw new Error(result.error);\n  }\n  return { json: result };\n} catch (e) {\n  throw new Error(`Failed to parse Python script output: ${e.message}. Stdout: ${stdout}`);\n}"
      },
      "id": "parse-merge-output",
      "name": "Parse Merge Output",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        11080,
        4112
      ]
    },
    {
      "parameters": {
        "jsCode": "const item = $input.first().json;\nconst criteria = item.evaluationCriteria || {};\nconst content = item.fullDocument;\n\nconst prompt = `You are a compliance officer. Evaluate this document.\\n\\nDOCUMENT:\\n${content.substring(0, 15000)}\\n\\nCRITERIA:\\n${JSON.stringify(criteria, null, 2)}\\n\\nProvide JSON output: { \"overallScore\": number, \"criteriaScores\": { key: { score, evidence, gaps } }, \"summary\": string, \"recommendations\": [] }.`;\n\nconst requestBody = {\n  model: \"llama3.2\",\n  prompt: prompt,\n  format: \"json\",\n  stream: false\n};\n\nconst jsonString = JSON.stringify(requestBody);\nconst buffer = Buffer.from(jsonString, 'utf8');\nconst filePrefix = item.filePrefix || 'unknown_';\nconst fileName = `${filePrefix}ollama_req.json`;\nconst fullPath = `/tmp/n8n_processing/${fileName}`;\n\nreturn [{\n  json: {\n    ...item,\n    ollamaRequestPath: fullPath,\n    fileName: fileName\n  },\n  binary: {\n    data: {\n      data: buffer.toString('base64'),\n      mimeType: 'application/json',\n      fileName: fileName\n    }\n  }\n}];"
      },
      "id": "11111111-ai01-node-build-prompt",
      "name": "Prepare Ollama Request",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        11280,
        4112
      ]
    },
    {
      "parameters": {
        "operation": "write",
        "fileName": "={{ $json.ollamaRequestPath }}",
        "options": {}
      },
      "id": "write-ollama-request",
